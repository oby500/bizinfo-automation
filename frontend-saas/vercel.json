{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["icn1"],
  "env": {
    "POSTGRES_URL": "@postgres_url",
    "AUTH_SECRET": "@auth_secret",
    "BASE_URL": "@base_url",
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@next_public_supabase_anon_key",
    "NEXT_PUBLIC_PORTONE_STORE_ID": "@next_public_portone_store_id",
    "PORTONE_API_SECRET": "@portone_api_secret",
    "NEXT_PUBLIC_GOOGLE_CLIENT_ID": "@next_public_google_client_id",
    "NEXT_PUBLIC_GOOGLE_CLIENT_SECRET": "@next_public_google_client_secret",
    "NEXT_PUBLIC_KAKAO_CLIENT_ID": "@next_public_kakao_client_id",
    "NEXT_PUBLIC_KAKAO_CLIENT_SECRET": "@next_public_kakao_client_secret",
    "NEXT_PUBLIC_NAVER_CLIENT_ID": "@next_public_naver_client_id",
    "NEXT_PUBLIC_NAVER_CLIENT_SECRET": "@next_public_naver_client_secret",
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/backend/:path*",
      "destination": "https://api.roten.kr/:path*"
    }
  ]
}
